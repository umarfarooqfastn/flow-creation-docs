          "lambdaFunction": {
            "code": "import jwt # PyJWT\nfrom jwt import InvalidTokenError\n\ndef fastn_function(params):\n    # Assuming the JWT token is included in the params for simulation\n    auth_header = params[\"data\"][\"headers\"][\"authorization\"]\n    if not auth_header.startswith('Bearer '):\n        return {\"error\": \"Missing or invalid Authorization header\"}\n\n    token = auth_header.replace('Bearer ', '')\n\n    # Your BigCommerce app's client secret\n    CLIENT_ID = params[\"data\"][\"secrets\"][\"bc_client_id\"]\n    CLIENT_SECRET = params[\"data\"][\"secrets\"][\"bc_client_secret\"]\n\n    try:\n        # Verify and decode the token\n        payload = jwt.decode(token, CLIENT_SECRET, audience=CLIENT_ID, algorithms=['HS512'])\n\n        # Extract customer email and store hash from the payload\n        email = payload.get('customer', {}).get('email')\n        store_hash = payload.get('store_hash')\n\n        if not email:\n            return {\"error\": \"Email not found in token\"}\n\n        # Return valid result\n        return {\n            \"status\": \"success\",\n            \"email\": email,\n            \"store_hash\": store_hash\n        }\n\n    except InvalidTokenError as e:\n        return {\"error\": f\"Token verification failed: {str(e)}\"}",
